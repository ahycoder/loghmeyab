<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/consLayToolbar"
  android:layout_width="match_parent"
  android:layout_height="?actionBarSize"
  android:background="@color/colorPrimary"
  android:orientation="vertical">

  <ImageView
    android:id="@+id/imgToolbarMenu"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginEnd="16dp"
    android:layout_marginRight="16dp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:srcCompat="@drawable/ic_menu_white_24dp"
    tools:ignore="ContentDescription,MissingConstraints" />

  <ImageView
    android:id="@+id/imgToolbarLocation"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="8dp"
    android:layout_marginLeft="8dp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintStart_toEndOf="@+id/imgToolbarMessage"
    app:layout_constraintTop_toTopOf="parent"
    app:srcCompat="@drawable/ic_location_on_white_24dp"
    tools:ignore="ContentDescription,MissingConstraints" />

  <ImageView
    android:id="@+id/imgToolbarMessage"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="16dp"
    android:layout_marginLeft="16dp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:srcCompat="@drawable/ic_message_white_24dp"
    tools:ignore="ContentDescription,MissingConstraints" />

  <ImageView
    android:id="@+id/imageView"
    android:layout_width="120dip"
    android:layout_height="wrap_content"
    app:layout_constraintEnd_toStartOf="@+id/imgToolbarMenu"
    app:layout_constraintStart_toEndOf="@+id/imgToolbarLocation"
    app:srcCompat="@drawable/title_toolbar" />

</androidx.constraintlayout.widget.ConstraintLayout>